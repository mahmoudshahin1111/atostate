{
  "name": "atostate",
  "version": "0.1.3",
  "description": "A lightweight state management library for JavaScript and TypeScript.",
  "license": "MIT",
  "author": "Mahmoud Shahin",
  "repository": {
    "type": "git",
    "url": "https://github.com/mahmoudshahin1111/atostate"
  },
  "homepage": "https://github.com/mahmoudshahin1111/atostate#readme",
  "bugs": {
    "url": "https://github.com/mahmoudshahin1111/atostate/issues"
  },
  "type": "commonjs",
  "sideEffects": false,
  "main": "dist/index.ts",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c ",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "state",
    "state-management",
    "store",
    "global-state",
    "reactive",
    "observable",
    "pubsub",
    "flux",
    "redux-alternative",
    "zustand-alternative",
    "lightweight",
    "tiny",
    "typescript",
    "javascript",
    "framework-agnostic",
    "frontend"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rollup": "^4.54.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write"
    ]
  }
}
